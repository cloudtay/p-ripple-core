{
  "name": "cloudtay/p-ripple-core",
  "version": "v0.4.2",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Psc\\": "src/",
      "P\\": "src/Supports/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "cclilshy",
      "email": "jingnigg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-posix": "*",
    "ext-sockets": "*",
    "ext-pcntl": "*",
    "ext-openssl": "*",
    "revolt/event-loop": "^1.0",
    "symfony/http-foundation": "*",
    "guzzlehttp/guzzle": "*",
    "psr/http-message": "*"
  },
  "require-dev": {
    "ext-curl": "*",
    "ext-parallel": "*",
    "phpunit/phpunit": "^11.2",
    "friendsofphp/php-cs-fixer": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
